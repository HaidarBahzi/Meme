{
  "schemaVersion": 1,
  "id": "meme",
  "version": "${version}",
  "name": "Meme",
  "description": "Play meme sounds in Minecraft with a hotkey.",
  "authors": ["HaidarBahzi"],
  "contact": {
    "homepage": "https://github.com/HaidarBahzi/Meme",
    "sources": "https://github.com/HaidarBahzi/Meme"
  },
  "license": "MIT",
  "icon": "assets/meme/icon.png",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "haidarbahzi.meme.client.MemeDataGenerator"
    ],
    "client": [
      "haidarbahzi.meme.client.MemeClient"
    ],
    "main": [
      "haidarbahzi.meme.Meme"
    ]
  },
  "mixins": [
    "meme.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
